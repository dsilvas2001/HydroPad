{
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 9098
    },
    "hosting": {
      "port": 9097
    },
    "ui": {
      "enabled": true,
      "port": 9096
    }
  },
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "public": "dist/hydropad",
    "rewrites": [
      {
        "destination": "/index.html",
        "source": "**"
      }
    ],
    "target": "hydropad"
  }
}